{
  "name": "ProductsRecommendationOutputTable",
  "properties": {
    "structure": [
      {
        "name": "CustomerId",
        "type": "Int"
      },
      {
        "name": "RecommendedProductId",
        "type": "Int"
      }
    ],
    "type": "AzureBlob",
    "linkedServiceName": "StorageLinkedService",
    "typeProperties": {
      "folderPath": "productrec/recommendationsoutput/yearno={Year}/monthno={Month}/",
      "partitionedBy": [
        {
          "name": "Year",
          "value": {
            "type": "DateTime",
            "date": "SliceStart",
            "format": "yyyy"
          }
        },
        {
          "name": "Month",
          "value": {
            "type": "DateTime",
            "date": "SliceStart",
            "format": "%M"
          }
        }
      ],
      "format": {
        "type": "TextFormat",
        "columnDelimiter": "\t"
      }
    },
    "availability": {
      "frequency": "month",
      "interval": 1
    }
  }
}
